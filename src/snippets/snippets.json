    "body": ["{% decrement ${1:variable} %}"]
  },
  "Tag capture": {
    "prefix": "capture",
    "description": "Variable tag: capture",
    "body": ["{% capture ${1:variable} %}$2{% endcapture %}"]
  },
  "Tag include": {
    "prefix": "include",
    "description": "Theme tag: include",
    "body": ["{% include '${1:snippet}' %}"]
