  "Tag section": {
    "prefix": "section",
    "description": "Theme tag: section",
    "body": ["{% section '${1:snippet}' %}"]
  },
  "Tag raw": {
    "prefix": "raw",
    "description": "Theme tag: raw",
    "body": ["{% raw %}$1{% endraw %}"]
  },
  "Tag layout": {
    "prefix": "layout",
